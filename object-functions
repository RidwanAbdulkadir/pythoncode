#object functions
class Student:
    def __init__(self, name, major, gpa):
        self.name = name
        self.major = major
        self.gpa = gpa

    def on_honour_roll(self):
        if self.gpa >= '3.5':
            return True
        else:
            return False






#object
from pycode import Student

student1 = Student('marie','arts','3.1')
student2 = Student('audu','computer sscience','3.9')

print(student1.on_honour_roll())









