import random

feet_in_mile = 5280
meters_kilometer = 1000
beatles = ['john','paul','mcCartney','george harrison','ringo star']

def _file_ext(filename):
    return filename[filename.index('.') + 1:]

def roll_dice(num):
    return random.randint(1,num)
