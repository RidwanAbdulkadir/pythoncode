#2 dimensional list
number_grid = [
    [9,8,7,6,5],
    [9,9,9,9,9],
    [4,1,3,8,9],
    [2,6,0,3,7],
    [4,6,2,1,5]
]

print(number_grid[0][1])

#nested for loops
number_grid = [
    [9,8,7,6,5],
    [9,9,9,9,9],
    [4,1,3,8,9],
    [2,6,0,3,7],
    [4,6,2,1,5]
]

for row in number_grid:
    for col in row:
        print(col)

